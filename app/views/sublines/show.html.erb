<div class="page-header">
  <%= link_to sublines_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_subline_path(@subline), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show subline</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @subline.name %></dd>

  <dt>Code:</dt>
  <dd><%= @subline.code %></dd>

  <dt>Description:</dt>
  <dd><%= @subline.description %></dd>

  <dt>Line:</dt>
  <dd><%= @subline.line %></dd>

  <dt>Enable:</dt>
  <dd><%= @subline.enable %></dd>

</dl>
