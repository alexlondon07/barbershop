<div class="page-header">
  <%= link_to lines_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_line_path(@line), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show line</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @line.name %></dd>

  <dt>Code:</dt>
  <dd><%= @line.code %></dd>

  <dt>Description:</dt>
  <dd><%= @line.description %></dd>

  <dt>Enable:</dt>
  <dd><%= @line.enable %></dd>

</dl>
