<div class="page-header">
  <%= link_to lines_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    <%= t ('app.back') %>
  <% end %>
  <%= link_to edit_line_path(@line), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    <%= t ('app.edit') %>
  <% end %>
  <h1><%= t 'lines.show.title' %></h1>
</div>

<dl class="dl-horizontal">

  <dt><%= t('lines.barber_shop') %>:</dt>
  <dd><%= @line.barber_shop.name %></dd>

  <dt><%= t 'lines.name' %>:</dt>
  <dd><%= @line.name %></dd>

  <dt><%= t 'lines.code' %>:</dt>
  <dd><%= @line.code %></dd>

  <dt><%= t 'lines.description' %>:</dt>
  <dd><%= @line.description %></dd>

  <dt><%= t 'lines.enable' %>:</dt>
  <dd><%= @line.enable %></dd>

</dl>
